{
  "name": "hono-leveldb-logger",
  "version": "1.0.0",
  "description": "Comprehensive logging library with global regulatory compliance and microservice authentication using Hono and LevelDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:auth": "vitest run tests/auth",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm run test",
    "semantic-release": "semantic-release",
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts"
  },
  "keywords": [
    "hono",
    "leveldb",
    "logging",
    "compliance",
    "gdpr",
    "appi",
    "pci-dss",
    "authentication",
    "microservices",
    "jwt",
    "mtls",
    "api-key",
    "security"
  ],
  "author": "Developer",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.0.0",
    "level": "^8.0.0",
    "uuid": "^9.0.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "semantic-release": "^22.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/hono-leveldb-logger.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/hono-leveldb-logger/issues"
  },
  "homepage": "https://github.com/yourusername/hono-leveldb-logger#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}

